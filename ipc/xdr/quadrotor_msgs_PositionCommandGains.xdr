%{
#include <cstring>
#include <ipc_bridge/msgs/std_msgs_Header.h>
#include <ipc_bridge/msgs/geometry_msgs_Vector3.h>
%}

ipc_type std_msgs_Header;
ipc_type geometry_msgs_Vector3;

struct quadrotor_msgs_PositionCommandGains
{
 std_msgs_Header header;
 geometry_msgs_Vector3 kp;
 geometry_msgs_Vector3 kd;

%{
  quadrotor_msgs_PositionCommandGains()
  {
  }
  quadrotor_msgs_PositionCommandGains(const quadrotor_msgs_PositionCommandGains& msg)
  {
    header = msg.header;
    kp = msg.kp;
    kd = msg.kd;
  }
  ~quadrotor_msgs_PositionCommandGains()
  {
  }
  quadrotor_msgs_PositionCommandGains& operator= (const quadrotor_msgs_PositionCommandGains& msg)
  {
    header = msg.header;
    kp = msg.kp;
    kd = msg.kd;

    return *this;
  }
%}

};

%{
namespace ipc_bridge
{
    namespace quadrotor_msgs
    {
        typedef quadrotor_msgs_PositionCommandGains PositionCommandGains;
    }
}
%}
